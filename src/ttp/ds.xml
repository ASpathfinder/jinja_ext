<group name="assign_str">
{{ var }}{{ eq | re("\s*=\s*") | strip(' ') }}{{ value | re("\"(.*)\"") | strip('"') }}
</group>

<group name="unit">
unit {{ name }}{{ eq | re("(\s*=\s*)*") | strip(' ') }}{{ command | re("(\".*\")*") | strip('"') }}
</group>


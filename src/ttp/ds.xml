<group name="assign_numeric">
## {{ var }}{{ eq | re("\s*=\s*") }}{{ value | re("[\d\.]+") }}
{{ var }}{{ eq | re("\s*=\s*") | strip(' ') }}{{ value | re("[\d\.]+") }}{{ unit | re("(\s+[^\s^,]+)*") }}{{ comma | re("(\s*,\s*)*") | strip(' ') }}{{ command | re("(\"(.*)\")*") | strip('"') }}
</group>

